<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Find_Team"
    android:background="@color/eerie_black"
    android:id="@+id/findTeamPage">

    <ImageView
        android:id="@+id/dashbarLine"
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:scaleX="30"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Dashbar"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@android:drawable/arrow_down_float" />

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/Dashbar"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@color/darkest_grey"
        android:minHeight="?attr/actionBarSize"
        android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:popupTheme="@style/ThemeOverlay.AppCompat.Dark.ActionBar" />

    <TextView
        android:id="@+id/createTeamtxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/createTeamtxt"
        android:textColor="@color/davy_grey"
        android:textColorHighlight="@color/white"
        android:textColorHint="@color/davy_grey"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.133"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.129" />

    <Button
        android:id="@+id/createTeambtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/createtxt"
        android:textColor="@color/white"
        android:textColorHighlight="@color/davy_grey"
        android:textColorHint="@color/eerie_black"
        app:backgroundTint="@color/lava"
        app:iconTint="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.908"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.13" />

    <ImageView
        android:id="@+id/fisrtTeamBackground"
        android:layout_width="353dp"
        android:layout_height="86dp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.275"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.3"
        app:srcCompat="@color/darkest_grey" />

    <ImageView
        android:id="@+id/fourthTeamBackground"
        android:layout_width="353dp"
        android:layout_height="86dp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.275"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/thirdTeamBackground"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@color/darkest_grey" />

    <TextView
        android:id="@+id/firstTeamInListName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Team1"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.05"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.278" />

    <TextView
        android:id="@+id/firstTeamInListGame"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="T1Game"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.053"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.352" />

    <Button
        android:id="@+id/joinFirstTeam"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="10dp"
        android:text="@string/joinTeamtxt"
        android:textColor="@color/white"
        android:textColorHighlight="@color/davy_grey"
        android:textColorHint="@color/eerie_black"
        app:backgroundTint="@color/lava"
        app:iconTint="@color/white"
        app:layout_constraintBottom_toBottomOf="@+id/fisrtTeamBackground"
        app:layout_constraintEnd_toEndOf="@+id/fisrtTeamBackground"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/fisrtTeamBackground" />

    <ImageView
        android:id="@+id/secondTeamBackground"
        android:layout_width="353dp"
        android:layout_height="86dp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.275"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fisrtTeamBackground"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@color/darkest_grey" />

    <TextView
        android:id="@+id/secondTeamInListGame"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="T2Game"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.053"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.482" />

    <Button
        android:id="@+id/joinSecondTeam"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="10dp"
        android:text="@string/joinTeamtxt"
        android:textColor="@color/white"
        android:textColorHighlight="@color/davy_grey"
        android:textColorHint="@color/eerie_black"
        app:backgroundTint="@color/lava"
        app:iconTint="@color/white"
        app:layout_constraintBottom_toBottomOf="@+id/secondTeamBackground"
        app:layout_constraintEnd_toEndOf="@+id/secondTeamBackground"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/secondTeamBackground" />

    <TextView
        android:id="@+id/secondTeamInListName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Team2"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.05"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.416" />

    <ImageView
        android:id="@+id/thirdTeamBackground"
        android:layout_width="353dp"
        android:layout_height="86dp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.275"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/secondTeamBackground"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@color/darkest_grey" />

    <TextView
        android:id="@+id/thirdTeamInListName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Team3"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.047"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.553" />

    <TextView
        android:id="@+id/fourthTeamInListName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Team4"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.047"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.691" />

    <TextView
        android:id="@+id/thirdTeamInListGame"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="T3Game"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.053"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.628" />

    <TextView
        android:id="@+id/fourthTeamInListGame"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="T4Game"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.053"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.766" />

    <Button
        android:id="@+id/joinThirdTeam"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="10dp"
        android:text="@string/joinTeamtxt"
        android:textColor="@color/white"
        android:textColorHighlight="@color/davy_grey"
        android:textColorHint="@color/eerie_black"
        app:backgroundTint="@color/lava"
        app:iconTint="@color/white"
        app:layout_constraintBottom_toBottomOf="@+id/thirdTeamBackground"
        app:layout_constraintEnd_toEndOf="@+id/thirdTeamBackground"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/thirdTeamBackground" />

    <Button
        android:id="@+id/joinFourthTeam"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="10dp"
        android:text="@string/joinTeamtxt"
        android:textColor="@color/white"
        android:textColorHighlight="@color/davy_grey"
        android:textColorHint="@color/eerie_black"
        app:backgroundTint="@color/lava"
        app:iconTint="@color/white"
        app:layout_constraintBottom_toBottomOf="@+id/fourthTeamBackground"
        app:layout_constraintEnd_toEndOf="@+id/fourthTeamBackground"
        app:layout_constraintHorizontal_bias="0.985"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/fourthTeamBackground" />

    <TextView
        android:id="@+id/joinExistingTeamtxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/joinExistingTeamtxt"
        android:textSize="30sp"
        android:textColor="@color/davy_grey"
        android:textColorHighlight="@color/white"
        android:textColorHint="@color/davy_grey"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.14"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.222" />

    <TextView
        android:id="@+id/pageNumbertxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1"
        android:textColor="@color/davy_grey"
        android:textColorHighlight="@color/white"
        android:textColorHint="@color/davy_grey"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/nextPagebtn"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toEndOf="@+id/prevPagebtn"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.881" />

    <Button
        android:id="@+id/nextPagebtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:text="@string/nextPage"
        android:textColor="@color/white"
        android:textColorHighlight="@color/davy_grey"
        android:textColorHint="@color/eerie_black"
        app:backgroundTint="@color/lava"
        app:iconTint="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.869"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.891" />

    <Button
        android:id="@+id/prevPagebtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:text="@string/prevPage"
        android:textColor="@color/white"
        android:textColorHighlight="@color/davy_grey"
        android:textColorHint="@color/eerie_black"
        app:backgroundTint="@color/lava"
        app:iconTint="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.052"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.891" />

</androidx.constraintlayout.widget.ConstraintLayout>